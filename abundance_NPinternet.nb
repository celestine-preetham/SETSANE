       RowBox[{"{", 
        RowBox[{"\"\<ground\>\"", "<->", "#"}], "}"}], "&"}], "/@", 
      RowBox[{"Pick", "[", 
       RowBox[{"signals", ",", "wO", ",", 
        RowBox[{"_", "?", "Positive"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"wC", "=", 
     RowBox[{"ConstantArray", "[", 
      RowBox[{"0", ",", 
       RowBox[{"{", 
        RowBox[{"Ni", ",", "Nc"}], "}"}]}], "]"}]}], ";", " ", 
    RowBox[{"(*", " ", 
